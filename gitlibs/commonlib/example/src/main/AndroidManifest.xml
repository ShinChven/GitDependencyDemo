          package="com.github.ShinChven.lib.CommonLib.example">

    <application
            android:allowBackup="true"
            android:name="com.github.ShinChven.CommonLib.example.AppContext"
            android:icon="@drawable/ic_launcher"
            android:label="@string/app_name"
            android:theme="@style/AppThemeTeal">
        <activity
                android:name="com.github.ShinChven.CommonLib.example.MainActivity"
                android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>
